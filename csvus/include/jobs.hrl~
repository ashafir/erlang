-record(jobsparm,
        {
          action = "all",
          status = "all",
          date1 = qdate:to_string("Y-01-01"),
          date2 = qdate:to_string("Y-m-d"),
          project = [],
          draw = [],
          user = [],
          org = [],
          nrec   = 32
        }).

-record(job,
        {jobid,
         action,
         status,
         datecreated,
         datestarted,
         datecompleted,
         timeelapsed,
         timeexecuted,
         userid,
         firstname,
         lastname,
         orgid,
         orgname,
         projectid,
         projectnumber,
         projectname,
         drawrequestid,
         drawnumber,
         auditfilepath,
         contentfilepath,
         resultsfilepath}).
